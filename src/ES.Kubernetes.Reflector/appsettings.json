{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "LevelSwitches": { "$consoleLevelSwitch": "Verbose" },
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System.Net.Http": "Warning",
        "Polly": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Error",
        "ES.FX": "Information",
        "ES.Kubernetes.Reflector": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] ({SourceContext}) {Message:lj}{NewLine}{Exception}",
          "levelSwitch": "$consoleLevelSwitch"
        }
      }
    ]
  },
  "Ignite": {
    "Settings": {
      "Configuration": {
        "AdditionalJsonSettingsFiles": [],
        "AdditionalJsonAppSettingsOverrides": [ "overrides" ]
      },
      "OpenTelemetry": {
        "AspNetCoreTracingHealthChecksRequestsFiltered": true
      }
    },
    "OpenTelemetry": {
      "Exporter": {
        "Seq": {
          "Settings": {
            "Enabled": false
          }
        }
      }
    }
  },
  "Reflector": {
    "Watcher": {
      "Timeout": ""
    },
    "Kubernetes": {
      "SkipTlsVerify": false
    }
  }
}